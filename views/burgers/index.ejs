<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Burger Name</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% burgers.forEach(function(b) { %>
        <tr>
          <td><%= b.name %></td>
          <td><a href="/burgers/<%= b._id %>">Details</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>


<%- include('../partials/footer') %>